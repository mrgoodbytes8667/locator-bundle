{
    "name": "mrgoodbytes8667/locator-bundle",
    "description": "A Symfony bundle for location handlers",
    "type": "symfony-bundle",
    "license": "CC-BY-NC-4.0",
    "require": {
        "php": "^8.0",
        "symfony/config": "^5.3",
        "symfony/dependency-injection": "^5.3",
        "symfony/framework-bundle": "^5.3"
    },
    "require-dev": {
        "mrgoodbytes8667/test-common": ">=0.0.17",
        "symfony/test-pack": "^1.0.8",
        "symfony/var-dumper": "^5.3"
    },
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@goodbytes.live"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "conflict": {
        "doctrine/annotations": "<1.11.1",
        "doctrine/cache": "<1.10.2",
        "doctrine/collections": "<1.6.7",
        "doctrine/event-manager": "<1.1.1",
        "doctrine/inflector": "<1.4.3",
        "doctrine/lexer": "<1.2.1",
        "doctrine/persistence": "<2.1.0",
        "phpunit/phpunit": "<9.5",
        "spatie/enum": "<3.7.2",
        "symfony/property-access": "<5.3.0",
        "symfony/property-info": "<5.3.0",
        "symfony/serializer": "<5.2.2"
    },
    "autoload": {
        "psr-4": {
            "Bytes\\LocatorBundle\\": "src/",
            "Bytes\\LocatorBundle\\Tests\\": "Tests/"
        }
    }
}
